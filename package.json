{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "yarn codegen && env-cmd -f .env react-scripts start",
    "start:dev": "yarn fetch-env development && yarn start",
    "start:staging": "yarn fetch-env staging && yarn start",
    "fetch-env": "bin/fetchEnv.sh client",
    "build": "yarn codegen; react-scripts build",
    "clean": "rimraf dist",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "yarn run eslint src/",
    "lint:fix": "yarn run eslint --fix src/",
    "gen:theme-typings": "yarn chakra-cli tokens src/theme/index.ts",
    "codegen": "yarn graphql-codegen --config codegen.yml",
    "prepare": "husky install",
    "cypress:open": "cypress open",
    "test-staging:chrome": "cypress run --browser chrome --env environment=staging",
    "test-staging:debug": "cypress open --browser chrome --env environment=staging",
    "test-local:chrome": "cypress run --browser chrome --env environment=local",
    "test-local:debug": "cypress open --browser chrome --env environment=local"
  },
